# http://editorconfig.org

root = true

[*.java]
charset = utf-8
end_of_line = lf
indent_style = tab
indent_size = 2

[*.gradle]
charset = utf-8
end_of_line = lf
indent_style = tab
indent_size = 2

[*.yaml]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4

[*.py]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
